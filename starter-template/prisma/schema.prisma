generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

model To Do List {
    id        Int      @id @default(autoincrement())
    name      String   @unique
    toDo      String
    notDone   Boolean
    done      Boolean
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
