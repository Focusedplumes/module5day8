generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

model ToDoList {
    id        Int      @id @default(autoincrement())
    name      String   @unique
    description String
    notDone   Boolean  @default(false)
    done      Boolean  @default(true)
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
